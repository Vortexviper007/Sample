    <webLinks>
        <fullName>blng__Generate_Invoice</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Re-run Generate Invoice</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}

var queryResults = sforce.connection.query(&quot;SELECT NamespacePrefix FROM ApexClass WHERE Name = &apos;BatchAPI&apos;&quot;);

records = queryResults.getArray(&quot;records&quot;); 
if (records.length &gt; 0)
{
	var prefixClass = records[0].NamespacePrefix? records[0].NamespacePrefix + &apos;.&apos;: &apos;&apos;;
	var result = sforce.apex.execute
				(
					prefixClass + &quot;BatchAPI&quot;,
					&quot;invokeBatchInvoice&quot;,
					{invoiceRunId: &quot;{!blng__InvoiceRun__c.Id}&quot;}
				);
	alert(result);
}
document.location.reload(true);</url>
    </webLinks>
