    <fields>
        <fullName>blng__StatusIndicator__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF ( 
	OR (
		ISPICKVAL(blng__Status__c, &apos;Started&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 1/8 Processing&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 1/8 Completed&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 2/8 Processing&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 2/8 Completed&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 3/8 Processing&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 3/8 Completed&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 4/8 Processing&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 4/8 Completed&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 5/8 Processing&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 5/8 Completed&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 6/8 Processing&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 6/8 Completed&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 7/8 Processing&apos;), 
                ISPICKVAL(blng__Status__c, &apos;Step 7/8 Completed&apos;), 
		ISPICKVAL(blng__Status__c, &apos;Step 8/8 Processing&apos;)
	),
	IMAGE(&apos;/resource/blng__InvoiceRun_StatusOrange&apos;, &apos;In Progress&apos;, 20, 20),
	IF (
		ISPICKVAL(blng__Status__c, &apos;Completed with Errors&apos;),
		IMAGE(&apos;/resource/blng__InvoiceRun_StatusRed&apos;, &apos;In Progress&apos;, 20, 20),
		IF (
			ISPICKVAL(blng__Status__c, &apos;Completed&apos;),
			IMAGE(&apos;/resource/blng__InvoiceRun_StatusGreen&apos;, &apos;In Progress&apos;, 20, 20),
			&apos;&apos;
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
