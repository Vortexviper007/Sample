        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/apex.js&quot;)}

var prefix = &apos;&apos;;
var prefixNew = &apos;&apos;;
var result = sforce.connection.query(&apos;SELECT NamespacePrefix FROM ApexClass WHERE Name = \&apos;CommonUtilClass\&apos;&apos;);
var records = result.getArray(&quot;records&quot;);

for (var i=0; i&lt; records.length; i++) {
var record = records[i];
if(record.NamespacePrefix == &apos;invoiceit_s&apos;){
prefix =  record.NamespacePrefix+&apos;__&apos;; 
prefixNew = record.NamespacePrefix+&apos;/&apos;;
}
} 

var transactionQuery = sforce.connection.query(&quot;SELECT Name,&quot;+prefix+&quot;Payment_Method__r.&quot;+prefix+&quot;Card_Number__c,&quot;+prefix+&quot;Type__c,&quot;+prefix+&quot;Payment_Method__r.&quot;+prefix+&quot;Token__c,&quot;+prefix+&quot;Payment_Method__r.&quot;+prefix+&quot;Payment_Profile_ID__c,&quot;+prefix+&quot;Payment_Method__r.&quot;+prefix+&quot;Payment_Method__c  FROM &quot;+prefix+&quot;Transaction__c &quot; +
&quot; WHERE id = &apos;{!blng__PaymentTransaction__c.Id}&apos;&quot;);

var creditCardNo = &apos;&apos;;
var transactiontype = &apos;&apos;;
var brainTreeToken = &apos;&apos;;
var CybersourceToken = &apos;&apos;;
var paymentMethodType = &apos;&apos;;

var records = transactionQuery.getArray(&quot;records&quot;);
for (var i=0; i&lt; records.length; i++) {
if(prefix != &apos;&apos; &amp;&amp; prefix != null){ 
creditCardNo = records[0].Payment_Method__r.Card_Number__c;
transactiontype = records[0].Type__c;
brainTreeToken = records[0].Payment_Method__r.Token__c;
CybersourceToken = records[0].Payment_Method__r.Payment_Profile_ID__c;
paymentMethodType = records[0].Payment_Method__r.Payment_Method__c;
}else{
creditCardNo = records[0].Payment_Method__r.Card_Number__c;
transactiontype = records[0].Type__c;
brainTreeToken = records[0].Payment_Method__r.Token__c;
CybersourceToken = records[0].Payment_Method__r.Payment_Profile_ID__c;
paymentMethodType = records[0].Payment_Method__r.Payment_Method__c;
}
if(creditCardNo == null &amp;&amp; brainTreeToken == null &amp;&amp; CybersourceToken == null &amp;&amp; paymentMethodType == &apos;Credit Card&apos; ){
alert(&apos;credit card details are not available to process&apos;);
}else if(transactiontype == &apos;Charge&apos;) {
// Now make a synchronous call to the Apex Web service
// method
var result = sforce.apex.execute(
&quot;&quot;+prefixNew+&quot;PaymentOperations&quot;,
&quot;transactionRefund&quot;,
{transactionId : &quot;{!blng__PaymentTransaction__c.Id}&quot;});

result = result[0];
alert(result.message);

if(result.reloadPage == &quot;true&quot;) {
window.location.reload();
}
}else {
alert(&apos;We can not process this record&apos;);
}
}</url>
