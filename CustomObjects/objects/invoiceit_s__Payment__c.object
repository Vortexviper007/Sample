    <validationRules>
        <fullName>invoiceit_s__Cannot_change_status_from_Cancelled</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE
(invoiceit_s__Status__c), 
&quot;Cancelled&quot;) &amp;&amp; ISCHANGED(invoiceit_s__Status__c)</errorConditionFormula>
        <errorDisplayField>invoiceit_s__Status__c</errorDisplayField>
        <errorMessage>You cannot change the status of a cancelled payment</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>invoiceit_s__Cannot_change_the_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( invoiceit_s__Amount__c )</errorConditionFormula>
        <errorDisplayField>invoiceit_s__Amount__c</errorDisplayField>
        <errorMessage>You cannot change the amount of payments. Alternately you can cancel this payment and create a new payment with a different amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>invoiceit_s__Cheque_Details_valid_for_cheque_payments</fullName>
        <active>true</active>
        <description>The cheque details can be populated only for cheque payments</description>
        <errorConditionFormula>AND(!ISPICKVAL( invoiceit_s__Payment_Type__c , &quot;Cheque&quot;), OR( invoiceit_s__Cheque_Number__c != Null,  invoiceit_s__Cheque_Date__c  != null,  invoiceit_s__Bank_Name__c != null))</errorConditionFormula>
        <errorMessage>Cheque Number, Cheque Date and Bank Name can be populated only for the payment type &quot;Cheque&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>invoiceit_s__Cheque_Number_Req_For_Cheque_Payment</fullName>
        <active>true</active>
        <description>Cheque Number Required For Cheque Payment</description>
        <errorConditionFormula>IF( (ISPICKVAL( invoiceit_s__Payment_Type__c , &quot;Cheque&quot;)) &amp;&amp; (invoiceit_s__Cheque_Number__c == null) , true, false)</errorConditionFormula>
        <errorDisplayField>invoiceit_s__Cheque_Number__c</errorDisplayField>
        <errorMessage>Enter the Cheque number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>invoiceit_s__Invoice_Status_should_be_posted</fullName>
        <active>true</active>
        <errorConditionFormula>AND((invoiceit_s__Invoice__c != NULL), !(ISPICKVAL(invoiceit_s__Invoice__r.invoiceit_s__Invoice_Status__c, &apos;Posted&apos;)),ISNEW())</errorConditionFormula>
        <errorDisplayField>invoiceit_s__Invoice__c</errorDisplayField>
        <errorMessage>You can select only posted invoices</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>invoiceit_s__Payment_Amount_Validation</fullName>
        <active>true</active>
        <description>You cannot create payment if the payment amount is greater than invoice unpaid amount</description>
        <errorConditionFormula>AND((invoiceit_s__Invoice__c != NULL),(invoiceit_s__Amount__c &gt; invoiceit_s__Invoice__r.invoiceit_s__Unpaid_Amount__c), ISNEW() )</errorConditionFormula>
        <errorDisplayField>invoiceit_s__Amount__c</errorDisplayField>
        <errorMessage>Please make sure that  payment amount should be less than or equal to invoice unpaid amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>invoiceit_s__Payment_status_cannot_be_Cancelled</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW()  &amp;&amp;  ISPICKVAL(invoiceit_s__Status__c, &quot;Cancelled&quot;)</errorConditionFormula>
        <errorMessage>You cannot insert the payment status as Cancelled</errorMessage>
    </validationRules>
